{
  "scripts": {
    "test": "npm run test-docker-chrome",
    "test-chrome": "node_modules/nightwatch/bin/nightwatch --config nightwatch_chrome.conf.js --env chrome",
    "test-firefox": "node_modules/nightwatch/bin/nightwatch --config nightwatch_firefox.conf.js --env firefox",
    "test-phantomjs": "node_modules/nightwatch/bin/nightwatch --config nightwatch_phantomjs.conf.js --env phantomjs",
    "test-docker-chrome": "node_modules/nightwatch/bin/nightwatch --config docker-chrome-conf.js",
    "test-docker-firefox": "node_modules/nightwatch/bin/nightwatch --config docker-firefox-conf.js",
    "test-docker-phantomjs": "node_modules/nightwatch/bin/nightwatch --config docker-phantomjs-conf.js",
    "report-chrome": "node reports/chrome/create-html-report-chrome.js",
    "report-firefox": "node reports/firefox/create-html-report-firefox.js",
    "report-phantomjs": "node reports/phantomjs/create-html-report-phantomjs.js",
    "test-api": "./node_modules/mocha/bin/mocha ./tests/api --recursive --reporter mochawesome --reporter-options reportDir=reports/mochawesome/ --timeout 30000",
    "test-api-parallel": "./node_modules/mocha-parallel-tests/dist/bin/mocha-parallel-tests ./tests/api --recursive --max-parallel 8 --timeout 30000"
  },
  "name": "nightwatchjs-mocha-poc-automation",
  "version": "1.0.0",
  "description": "nightwatchjs mocha poc automation",
  "main": "nightwatch.conf.js",
  "devDependencies": {
    "nightwatch-base-page": "^1.2.1"
  },
